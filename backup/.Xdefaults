! FONTS
! $ fc-list lists available fonts
URxvt.letterSpace: -1

URxvt*font:                 xft:Monaco for Powerline:regular:size=8.6:antialias=false
URxvt*imFont:               xft:Monaco for Powerline:regular:size=8.6:antialias=false
URxvt*boldFont:             xft:Monaco for Powerline:bold:size=8.6:antialias=false
URxvt*italicFont:           xft:Monaco for Powerline:italic:size=8.6:antialias=false
URxvt*boldItalicFont:       xft:Monaco for Powerline:bold:italic:size=8.6:antialias=false

Xft.dpi: 96
Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: true
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: rgb

