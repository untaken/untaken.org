! URxvt.selection.pattern-0: \\|([^│|]+)\\|
URxvt.selection.pattern-0: \\│([^|]+)\\│
URxvt.urlLauncher:      /usr/bin/firefox
URxvt.matcher.button:   1
URxvt.perl-ext-common:  default,matcher,selection,clipboard

URxvt*urgentOnBell: true
URxvt.iso14755: False
URxvt.termName:             rxvt-256color
URxvt*geometry: 140x50
! URxvt.depth: 33
! URxvt.background: [80]#2C001E
urxvt*depth: 32
URxvt.background: [60]#000000
IRC*depth: 32
IRC.background: [65]#000000
Tmux1*depth: 32
Tmux1.background: [65]#000000
Tmux2*depth: 32
Tmux2.background: [65]#000000
! urxvt*background: rgba:0000/0000/0200/c800
URxvt*foreground:White
URxvt*cursorBlink: false
URxvt*borderLess: false
URxvt*cursorUnderline: true
URxvt.visualBell: false
URxvt*scrollBar: false
URxvt*scrollstyle: plain
URxvt*scrollBar_right: 1
URxvt*scrollTtyOutput: false
URxvt*scrollTtyKeypress: true
URxvt*secondaryScroll: true
URxvt*saveLines: 16000

Urxvt*foreground: #f2f2f2
Urxvt*background: #101010



! COLOR PALETTE
! black
URxvt.color0  : #2E2E34343636
URxvt.color8  : #555557575353
! red
URxvt.color1  : #CCCC00000000
URxvt.color9  : #EFEF29292929
! green
URxvt.color2  : #4E4E9A9A0606
URxvt.color10 : #8A8AE2E23434
! yellow
