Config { font = "xft:Monaco for Powerline:regular:size=8.4:antialias=false"
       , bgColor = "#303030"
       , fgColor = "#00afd7"
       , position = TopW L 100 
       , commands = [ Run Weather "EGPF" ["-t"," <tempC>C","-L","19","-H","24","--normal","green","--high","red","--low","lightblue"] 36000
                    , Run Network "eth0" ["-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Com "last_irc_message" [] "irc" 10 
                    , Run Date "%a %b %_d %l:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ <fc=red>%irc%</fc>  -   %eth0% %cpu% %memory% %swap% <fc=#5fd7d7>%date%</fc> %EGPF%                   "
       }
